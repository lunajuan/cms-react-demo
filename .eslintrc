{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "camelcase": "warn",
    "no-shadow": "warn",
    "arrow-body-style": "warn",
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-nested-ternary": 0,
    "react/prop-types": 1
  }
}